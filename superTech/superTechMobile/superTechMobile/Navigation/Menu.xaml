<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="superTechMobile.Navigation.Menu"
             xmlns:nav="clr-namespace:superTechMobile.Navigation"
            xmlns:pages="clr-namespace:superTechMobile.Views.Products"
            FlyoutLayoutBehavior="Popover"
            >
    <FlyoutPage.Flyout>
        <nav:MenuFlyout x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:ProductsPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>